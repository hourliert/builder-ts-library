{
  "name": "builder-ts-library",
  "version": "0.0.2",
  "description": "Builder Archetype - Typescript Library",
  "main": "lib/index.js",
  "scripts": {
    "builder:compile": "rimraf ./lib && babel index.js --out-dir lib/ && babel scripts --out-dir lib/scripts && babel config --out-dir lib/config",
    "builder:gen-dev": "builder-support gen-dev",
    "prepublish": "npm run builder:compile",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "eslint": "eslint --color 'src/**/*.{js,jsx}'",
    "lint": "builder concurrent eslint tslint",
    "test:all": "BABEL_ENV=commonjs jest --config node_modules/builder-ts-library/config/jest/config.json --colors",
    "test:all:watch": "BABEL_ENV=commonjs jest --config node_modules/builder-ts-library/config/jest/config.json --watch --watchExtensions ts,tsx,js --colors",
    "test:all:coverage": "BABEL_ENV=commonjs jest --config node_modules/builder-ts-library/config/jest/config.coverage.json --colors",
    "build:typescript": "rimraf ./libTs && tsc -p .",
    "build:commonjs": "gulp --cwd . --gulpfile node_modules/builder-ts-library/config/gulp/gulpfile.js build-es5",
    "build:es": "gulp --cwd . --gulpfile node_modules/builder-ts-library/config/gulp/gulpfile.js build-es2015",
    "build": "builder concurrent build:typescript build:commonjs build:es",
    "build:typescript:watch": "tsc --project . --watch",
    "build:commonjs:watch": "gulp --cwd . --gulpfile node_modules/builder-ts-library/config/gulp/gulpfile.js watch-es5",
    "build:es:watch": "gulp --cwd . --gulpfile node_modules/builder-ts-library/config/gulp/gulpfile.js watch-es2015",
    "build:watch": "builder concurrent build:typescript:watch build:commonjs:watch build:es:watch",
    "typings:install": "typings install",
    "release": "node node_modules/builder-ts-library/lib/index release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hourliert/builder-ts-library.git"
  },
  "keywords": [
    "builder",
    "archetype",
    "typescript",
    "library"
  ],
  "author": "Thomas Hourlier <hourliert@gmail.com> (https://github.com/hourliert)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hourliert/builder-ts-library/issues"
  },
  "homepage": "https://github.com/hourliert/builder-ts-library#readme",
  "dependencies": {
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-check-es2015-constants": "^6.5.0",
    "babel-plugin-transform-class-properties": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.5.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.5.0",
    "babel-plugin-transform-es2015-classes": "^6.5.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.5.0",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-duplicate-keys": "^6.5.0",
    "babel-plugin-transform-es2015-for-of": "^6.5.0",
    "babel-plugin-transform-es2015-function-name": "^6.5.0",
    "babel-plugin-transform-es2015-literals": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.0",
    "babel-plugin-transform-es2015-object-super": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.5.0",
    "babel-plugin-transform-es2015-spread": "^6.5.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.5.0",
    "babel-plugin-transform-es2015-template-literals": "^6.5.0",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.5.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-env": "^0.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.12.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "semver": "^5.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "builder-support": "^0.3.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-config-react": "^1.1.1",
    "eslint-plugin-react": "^4.1.0",
    "jscs": "^2.11.0"
  }
}
